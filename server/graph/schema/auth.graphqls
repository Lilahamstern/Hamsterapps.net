type User {
    id: ID!
    username: String!
    email: String!
}

input SignupInput {
    username: String!
    password: String!
}

input LoginInput {
    username: String!
    password: String!
}

type Query {
    users: [User!]!
}

type Mutation {
    signup(input: SignupInput!): String!
    login(input: LoginInput): String!
}



